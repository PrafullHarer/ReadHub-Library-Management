{
    "version": 2,
    "name": "readhub-library-management",
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        },
        {
            "source": "/src/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/login",
            "destination": "/src/pages/login.html"
        },
        {
            "source": "/contact",
            "destination": "/src/pages/contact.html"
        },
        {
            "source": "/welcome",
            "destination": "/src/pages/welcome.html"
        },
        {
            "source": "/admin",
            "destination": "/src/pages/admin-dashboard.html"
        },
        {
            "source": "/dashboard",
            "destination": "/src/pages/user-dashboard.html"
        },
        {
            "source": "/student",
            "destination": "/src/pages/user-dashboard.html"
        }
    ],
    "redirects": [
        {
            "source": "/admin-dashboard",
            "destination": "/admin",
            "permanent": true
        },
        {
            "source": "/user-dashboard",
            "destination": "/dashboard",
            "permanent": true
        }
    ]
}